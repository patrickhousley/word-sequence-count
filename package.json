{
  "name": "word-sequence-count",
  "version": "1.0.0",
  "description": "Counts the words of a text file",
  "main": "dist/index.js",
  "bin": "bin/cli.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "eslint",
    "format": "prettier --write .",
    "pretest": "rimraf coverage",
